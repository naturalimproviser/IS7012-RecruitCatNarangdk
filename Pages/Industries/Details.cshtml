@page
@model RecruitCatNarangdk.Pages.Industries.DetailsModel

@{
    ViewData["Title"] = "Details";
}

<h1 class="text-center alert alert-success">Industry: @Model.Industry.IndustryName</h1>

<div class="container">
    <div>
        <a asp-page="./Edit" asp-route-id="@Model.Industry.IndustryId">Edit</a> |
        <a asp-page="./Index">Back to List</a>
    </div>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Industry.IndustryName)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Industry.IndustryName)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Industry.IndustryLocation)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Industry.IndustryLocation)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Industry.ContactNumber)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Industry.ContactNumber)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Industry.EmailId)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Industry.EmailId)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Industry.IndustryFoundedDate)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Industry.IndustryFoundedDate)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Industry.IsPrivate)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Industry.IsPrivate)
        </dd>
    </dl>
</div>

